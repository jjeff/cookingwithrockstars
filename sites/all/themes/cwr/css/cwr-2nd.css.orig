n #content,#recipes #content {  background: url(../images/content_top_bar.png) top left no-repeat;}  #content-content {  min-height: 500px;  padding:20px 10px 20px 35px;  }#main {  float: left;  width: 469px;  }  #info {  float: right;  width: 160px;  margin-right: 13px;  display: inline;  }    #copyright {  clear: both;  padding:10px 35px 0 35px;  margin: 3em 0 0 0;  font-size: 95%;   color: #999;  height: 60px;  } #copyright {   background: #9acccc url(../images/content_bottom_white.gif) bottom no-repeat;   }#recipes #copyright,#kitchen #copyright,#shop #copyright,#discuss #copyright,#taxonomy #copyright {      background: #9acccc url(../images/content_bottom_bar.gif) bottom no-repeat;      }/*  ---------------------- END LAYOUT STYLES ----------------------------- *//*  ---------------------- SPECIFIC PAGE ELEMENT STYLES ---------------------- */#kitchen-head {   background: url(../images/jenskitchen_head.png) top left no-repeat;   width: 504px;   height: 112px;   text-indent: -999em;   font-size: .5em;   margin-top: -20px;   margin-left: -35px;   }#recipe-head {   background: url(../images/recipes_head.png) top left no-repeat;   width: 504px;   height: 112px;   text-indent: -999em;   font-size: .5em;   margin-top: -20px;   margin-left: -35px;   }/*  RECIPE STYLES (artist, recipes, and kitchen pages)   */.recipe-item {	font-family: Georgia, "Times New Roman", Times, serif;	font-size: 1.2em;	line-height: 1.4em;}	#main #recipe h2 {  background: url(../images/recipe.gif) no-repeat top left;  font-size: 1px;  width: 454px;  height: 32px;  text-indent: -999em;  margin: 0;  margin-top: 1em; }div.recipe-item {  margin-top: 2em;}.recipe-item h3 {  font-size: 1.5em;  font-weight: bold;  color: #366;  margin-bottom: .5em;  margin-top: .5em;}.recipe-item h4 {    font-family: Verdana, sans-serif;  text-transform: uppercase;  font-size: .8em;  letter-spacing: 2px;  color: